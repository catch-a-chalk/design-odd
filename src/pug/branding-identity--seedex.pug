extends layout/app

block page
  h1.visually-hidden Страница SEEDEX
  include chunk/header
  main
    section.template#template-seedex
      .template__grid
        .template__main-content
          .template__heading
            .template__heading-container
              h2 клиент
              p SeedEx
          .template__categories
            p брендинг
            p айдентика
          .template__services
            h3.template__h3 услуги
            ul.template__list
              li Аналитика
              li Платформа бренда
              li Фирменный стиль
              li Дизайн упаковки
              li Брендбук
        .template__pic
          img(src="img/t-seedex@1x.jpg", alt="Контейнеры SeedEx в виде сердца")
      .template__grid
        .template__main-content
          .template__subheading
            h3.template__h3 о проекте
            p.template__text При создании айдентики наши дизайнеры опирались на&nbsp;выводы по&nbsp;западной аналитике нового для России рынка CBD продукции, они легли в&nbsp;основу разработки концепции оптового импортёра инновационного товара из&nbsp;Европы. Нам было важно донести уникальность бренда&nbsp;&mdash; технологичность, сочетающуюся с&nbsp;фермерским трудом и&nbsp;природной пользой.
        .template__pic
          img(src="img/t-seedex-about@1x.jpg", alt="Тара SeedEx CBDM 5%")
      .template__grid
        .template__main-content
          .template__subheading
            h3.template__h3 логотип
            p.template__text Название SeedEX&nbsp;&mdash; это лаконичное сокращение Seed Extract, оно полностью отражает позиционирование и&nbsp;задачи бренда, отражённые в&nbsp;логотипе. В&nbsp;дизайне продуктов мы&nbsp;применили акцент на&nbsp;ярких цветах для удобства оклейки разного типа упаковки работниками склада. На&nbsp;данный момент линейка включает в&nbsp;себя 9&nbsp;товаров.
        .template__pic.template__pic--hands
          img(src="img/t-seedex-hands@1x.jpg", alt="Логотипы SeedEx")
        img(src="img/pencil-arrow.png", alt="Линия вниз").template__pencil-arrow
      .template__grid
        .template__pic.template__pic--colors
          img(src="img/t-seedex-colors@1x.jpg", alt="Цвета SeedEx")
        .template__pic
          img(src="img/t-seedex-pills@1x.jpg", alt="Упаковка SeedEx")
      .template__fullwidth
          img(src="img/t-seedex-booklet@1x.jpg", alt="Буклет SeedEx")
      .template__grid.template__grid--fullwidth
        img(src="img/t-seedex-specification@1x.jpg", alt="Спецификация SeedEx")
        img(src="img/t-seedex-card@1x.jpg", alt="Визитка SeedEx")
      .template__grid.template__grid--links
        .template__main-content
          .template__subheading
            p.template__text проект создан в 2022 году
        a(href="https://seedex.pro").template__link https://seedex.pro
      .template__grid.template__grid--arrows
        .template__main-content.template__main-content--footer
          a(href="index.html")
            img(src="img/icon-arrow.svg", atl="Назад", width="52").template__arrow.template__arrow--left
            h4 портфолио
        a(href="#template-seedex").template__up
          img(src="img/icon-arrow.svg", atl="Подняться", width="52").template__arrow.template__arrow--top
  //- include chunk/popup
  include chunk/footer
  .cursor
