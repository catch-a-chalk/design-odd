        //- .form__stages-modal#form__stages-modal
        //-   .form__stages-actions
        //-     button.btn-arrow(type='button')
        //-       svg(width="52" height="60" role="img" aria-labelledby="arrow__down" aria-description="Вниз")
        //-         use(href="img/sprite.svg#arrow")
        //-   each component in STAGES
        //-     h3=component.header
        //-     each element in component.data
        //-       h4=element.title
        //-       each checkBox in element.checkBoxes
        //-         .form__stages-content
        //-           h5=checkBox.span
        //-           p=checkBox.tooltip
        fieldset
          each element in component.data
            .form__boooooox
              h4=element.title
              p.form__stages-toggle#line=element.number
              .form__stages-list 
                each checkBox in element.checkBoxes
                  label(class=`form__label-stages-${checkBox.value}` for=`stages-${checkBox.value}`)
                    input(type='checkbox' name='stages' id=`stages-${checkBox.value}` data-span=`${checkBox.span}` value=checkBox.value)
                    span.form__option
                    =checkBox.span
                    span.form__stages-tooltip=checkBox.tooltip
      .form__stages-btn
        //- a.btn-white(href="order.html")
        button.btn-white(type="submit")
        a.btn-arrow(href="#inner2")
          svg(width="52" height="60" role="img" aria-labelledby="arrow__down" aria-description="Вниз")
            use(href="img/sprite.svg#arrow")


